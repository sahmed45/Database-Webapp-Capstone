@page
@model KSUWEBPMOCAP.Pages.StudentPortalModel
@{
    Layout = "/Pages/Shared/_Layout.cshtml";
    ViewData["Title"] = "StudentPortal";
}

    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>FacultyPortal</title>
    </head>
    <body>
        <h3>Student Sign In</h3>
        @Model.Msg
        <form method="post" asp-page="studeportal">
            <table>
                <tr>
                    <td>Net_ID</td>
                    <td>
                        <input asp-for="@Model.account.Student_Net_Id" />
                    </td>
                </tr>
                <tr>
                    <td>Password</td>
                    <td>
                        <input type="password" asp-for="@Model.account.Student_Password" />
                    </td>
                </tr>
                <tr>
                    <td>&nbsp;</td>
                    <td>
                        <input type="submit" value="Login" />
                    </td>
                </tr>
            </table>
        </form>
    </body>
    </html>
